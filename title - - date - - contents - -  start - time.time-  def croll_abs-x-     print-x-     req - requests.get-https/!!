
title = []
date = []
contents = []

start = time.time()

def croll_abs(x):
    print(x)
    req = requests.get('https://www.computer.org'+x)
    soup = bs(req.text, 'html.parser')
    title.append(soup.select('div.abstractTitle > a > h2')[0].text)
    contents.append(soup.select('div.abstractText.abstractTextMB')[0].text)
    date.append(x[15:22])

    
pool = Pool(processes=4)
pool.map(croll_abs,new_absLinks)

close = time.time()

print(close-start)
